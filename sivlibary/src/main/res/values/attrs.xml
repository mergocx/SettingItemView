<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SettingItem">
        <!--左图标的属性-->
        <attr name="isLeftIconCX" format="boolean"/>
        <attr name="leftIconIdCX" format="reference"/>
        <attr name="leftIconWidthCX" format="dimension"/>
        <attr name="leftIconHeightCX" format="dimension"/>
        <attr name="leftIconMarginLeftCX" format="dimension"/>
        <!--左文字的属性-->
        <attr name="isLeftTextViewCX" format="boolean"/>
        <attr name="leftTextCX" format="string"/>
        <attr name="leftTextMarginLeftCX" format="dimension"/>
        <!--main文字的属性-->
        <attr name="isMainTextViewCX" format="boolean"/>
        <attr name="mainTextCX" format="string"/>
        <attr name="mainTextMarginLeftCX" format="dimension"/>
        <attr name="mainTextMarginTopCX" format="dimension"/>
        <!--next文字的属性-->
        <attr name="isNextTextViewCX" format="boolean"/>
        <attr name="nextTextCX" format="string"/>
        <attr name="nextTextMarginBottomCX" format="dimension"/>
        <!--右文字的属性-->
        <attr name="isRightTextViewCX" format="boolean"/>
        <attr name="rightTextCX" format="string"/>
        <attr name="rightTextMarginRightCX" format="dimension"/>
        <!--右图标的属性-->
        <attr name="isRightIconCX" format="boolean"/>
        <attr name="rightIconIdCX" format="reference"/>
        <attr name="rightIconWidthCX" format="dimension"/>
        <attr name="rightIconHeightCX" format="dimension"/>
        <attr name="rightIconMarginRightCX" format="dimension"/>
        <!--ToggleButton的属性-->
        <attr name="isToggleButtonCX" format="boolean"/>
        <!--右边文字和图片的位置-->
        <attr name="isRightFirstCX" format="boolean"/>
    </declare-styleable>
</resources>
